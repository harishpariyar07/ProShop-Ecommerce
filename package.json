  "bugs": {
    "url": "https://github.com/harishpariyar07/ProShop-Ecommerce/issues"
  },
  "homepage": "https://github.com/harishpariyar07/ProShop-Ecommerce#readme",
  "dependencies": {
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "stripe": "^12.8.0",
    "uuid": "^9.0.0"
  },
